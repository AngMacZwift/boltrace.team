        <SteadyState Duration="240.00002" Power="1.2544999" pace="1128505344"/>
        <SteadyState Duration="30.000002" Power="1.0044999" pace="1128505344"/>
        <SteadyState Duration="240.00002" Power="0.6045" pace="1128505344"/>
        <SteadyState Duration="60" Power="1.5044999" pace="1128505344"/>
        <SteadyState Duration="30.000002" Power="1.0044999" pace="1128505344"/>
        <SteadyState Duration="150" Power="0.6045" pace="1128505344">
